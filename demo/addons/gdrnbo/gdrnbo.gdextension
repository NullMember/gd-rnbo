[configuration]

entry_symbol = "example_library_init"
compatibility_minimum = "4.1"

[libraries]
; Relative paths ensure that our GDExtension can be placed anywhere in the project directory.
macos.debug = "./macos/libgdrnbo.macos.template_debug.universal.dylib"
macos.release = "./macos/libgdrnbo.macos.template_release.universal.dylib"
ios.debug = "./ios/libgdrnbo.ios.template_debug.arm64.dylib"
ios.release = "./ios/libgdrnbo.ios.template_release.arm64.dylib"
windows.debug.x86_32 = "./windows/libgdrnbo.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "./windows/libgdrnbo.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "./windows/libgdrnbo.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "./windows/libgdrnbo.windows.template_release.x86_64.dll"
linux.debug.x86_64 = "./linux/libgdrnbo.linux.template_debug.x86_64.so"
linux.release.x86_64 = "./linux/libgdrnbo.linux.template_release.x86_64.so"
linux.debug.arm64 = "./linux/libgdrnbo.linux.template_debug.arm64.so"
linux.release.arm64 = "./linux/libgdrnbo.linux.template_release.arm64.so"
linux.debug.rv64 = "./linux/libgdrnbo.linux.template_debug.rv64.so"
linux.release.rv64 = "./linux/libgdrnbo.linux.template_release.rv64.so"
android.debug.x86_64 = "./android/libgdrnbo.android.template_debug.x86_64.so"
android.release.x86_64 = "./android/libgdrnbo.android.template_release.x86_64.so"
android.debug.arm64 = "./android/libgdrnbo.android.template_debug.arm64.so"
android.release.arm64 = "./android/libgdrnbo.android.template_release.arm64.so"
